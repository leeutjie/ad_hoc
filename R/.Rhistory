price_bs_euro_option <-
function(spot,
strike,
rf_rate,
vol,
ttmy,
div_yield,
type = "c") {
d1 <- (log(spot / strike) + (rf_rate - div_yield + vol ^ 2 / 2) * ttmy) / (vol * sqrt(ttmy))
d2 <- d1 - vol * sqrt(ttmy)
if (type == "c") {
spot * div_yield * pnorm(d1) - strike * exp(-rf_rate * ttmy) * pnorm(d2)
} else if (type == "p") {
strike * exp(-rf_rate * ttmy) * pnorm(-d2) - spot * pnorm(-d1)
}
}
